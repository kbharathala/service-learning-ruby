<p id="notice"><%= notice %></p>

<p>
  <strong>Place Name:</strong>
  <%= @service.placeName %>
</p>

<p>
  <strong>Address:</strong>
  <%= @service.placeAddres %>
</p>

<p>
  <strong>Phone Number:</strong>
  <%= @service.placePhone %>
</p>

<br> <br>

<p>
  <strong>Hours:</strong>
  <%= @service.hours %>
