<h1 id="log_in">Log In</h1>

  <%= form_tag(@user) do %>
    <div class="field">
      <%= label_tag :name %><br>
      <%= text_field_tag :name %>
    </div>
    <div class="field">
      <%= label_tag :password %><br>
      <%= password_field_tag :password %>
    </div>
    <div class="action">
      <%= submit_tag "Log In" %>
    </div>
  <% end %>
